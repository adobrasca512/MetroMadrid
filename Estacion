package MetroMadrid;

import java.util.Arrays;
import java.util.ArrayList;
public class Estacion {
private String nombreestacion;
private ArrayList<Accesos> accesos;
private String direccion [];
public Estacion(String nombreestacion, Accesos acceso) {//
	super();
	this.nombreestacion = nombreestacion;
	accesos=new ArrayList<Accesos> ();
	accesos.add(acceso);
}
public String getNombreestacion() {
	return nombreestacion;
}
public void setNombreestacion(String nombreestacion) {
	this.nombreestacion = nombreestacion;
}
public ArrayList<Accesos> getAccesos() {

	return accesos;
}
public void setAccesos(ArrayList<Accesos> accesos) {
	this.accesos = accesos;
}
public String getDireccion() {
	String inicio=getNombreestacion();
	String finales=getNombreestacion();
	//direccion[]=  {inicio,finales};
	return  Arrays.toString(direccion) ;
}

}
